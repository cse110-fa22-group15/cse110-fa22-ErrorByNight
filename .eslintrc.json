{
  "rules": {
    "accessor-pairs": "off",
    "selector-class-pattern": "off",
    "selector-id-pattern": "off"

  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true
  },
  "ignorePatterns": ["**/*.json", "node_modules/", "**/*.test.js", "out/", "**/*.Homepage.html"]
}
