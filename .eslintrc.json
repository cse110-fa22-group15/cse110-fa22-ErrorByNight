{
  "rules": {
    "accessor-pairs": "off",
    "kebab-case": "off",
    "selector-id-pattern": "^[a-z][a-zA-Z0-9]+$",
    "selector-class-pattern" : "^[a-z][a-zA-Z0-9]+$"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true
  },
  "ignorePatterns": ["**/*.json", "node_modules/"]
}
