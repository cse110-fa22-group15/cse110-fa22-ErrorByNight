{
  "rules": {
    "accessor-pairs": "off",
    "@typescript-superlint/kebabcase": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true
  },
  "ignorePatterns": ["**/*.json", "node_modules/"]
}
